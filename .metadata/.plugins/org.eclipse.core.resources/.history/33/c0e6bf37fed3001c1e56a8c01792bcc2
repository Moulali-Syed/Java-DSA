package oop;

public class WrapperExample {

	public static void main(String[] args) {

		int a = 10;
		int b = 20;
		Integer num = 45;

		swap(2, 3);
		System.out.println(a + " " + b);

	}

	static void swap(int a, int b) {
		int temp = a;
		a = b;
		b = temp;
		System.out.println(a + " " + b);
	}

}
