package theory;

import java.util.Scanner;

public class Calculato {

	public static void main(String[] args) {
	
		
		Scanner sc = new Scanner(System.in);
		while(true) {
			char op=sc.next().trim().charAt(0);
			if(op=='+' || op=='-' || op=='*' || op=='/' || op=='%') {
				//input two numbers
				
				int num1 = sc.nextInt();
				int num2 = sc.nextInt();
				
			}
		}

	}

}
