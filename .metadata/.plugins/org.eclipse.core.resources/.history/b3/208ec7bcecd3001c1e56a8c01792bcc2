package strings;

public class Comparision {

	public static void main(String[] args) {
		String a = "apple";
		String b = "Apple";
		System.out.println(a == b);
		
		//== comparator
		/*
		 * a-->points to Apple
		 * b-->points to Apple
		 * checks if reference variables pointing to same object
		 */

	}

}
